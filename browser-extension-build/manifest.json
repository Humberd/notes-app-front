{
  "manifest_version": 2,
  "name": "Notes App:dev",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "browser-extension/index.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "page": "background/index.html"
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:4200/*",
      "https://notes-app.humberd.pl/*"
    ]
  }
}

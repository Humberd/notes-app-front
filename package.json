{
  "name": "notes-app",
  "version": "6.2.0",
  "description": "Notes app",
  "homepage": "https://github.com/humberd/notes-app",
  "author": {
    "name": "Maciej Sawicki",
    "email": "humberd.dev@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 8",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build:browser-extension:dev": "ng build browser-extension",
    "build:browser-extension:dev-watch": "ng build browser-extension --watch",
    "build:browser-extension:prod": "ng build browser-extension --prod",
    "build:browser-extension-background:dev": "ng build browser-extension-background",
    "build:browser-extension-background:dev-watch": "ng build browser-extension-background --watch",
    "build:browser-extension-background:prod": "ng build browser-extension-background --prod",
    "build:browser-extension-content:dev": "ng build browser-extension-content",
    "build:browser-extension-content:dev-watch": "ng build browser-extension-content --watch",
    "build:browser-extension-content:prod": "ng build browser-extension-content --prod",
    "build:all-extensions-projects:dev-watch": "concurrently \"npm run build:browser-extension:dev-watch\" \"npm run build:browser-extension-background:dev-watch\" \"npm run build:browser-extension-content:dev-watch\"",
    "serve:web-app:web": "concurrently \"npm run translations:watch\" \"ng serve --host 0.0.0.0\"",
    "serve:browser-extension:web": "concurrently \"npm run translations:watch\" \"ng serve browser-extension --host 0.0.0.0 --port 4201\"",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "translations:watch": "node scripts/extract-translation-types.js",
    "bump-ng-boost": "npm i @ng-boost/core@latest @ng-boost/material@latest"
  },
  "dependencies": {
    "@angular/animations": "^9.0.6",
    "@angular/cdk": "~9.1.2",
    "@angular/cli": "^9.0.6",
    "@angular/common": "9.0.6",
    "@angular/compiler": "9.0.6",
    "@angular/compiler-cli": "9.0.6",
    "@angular/core": "9.0.6",
    "@angular/elements": "^9.0.6",
    "@angular/forms": "^9.0.6",
    "@angular/language-service": "9.0.6",
    "@angular/material": "^9.1.2",
    "@angular/platform-browser": "9.0.6",
    "@angular/platform-browser-dynamic": "9.0.6",
    "@angular/router": "9.0.6",
    "@ng-boost/core": "^0.9.0-beta.6",
    "@ng-boost/material": "^0.9.0-beta.6",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@webcomponents/custom-elements": "^1.3.2",
    "angular-resizable-element": "^3.3.0",
    "marked": "^0.8.0",
    "monaco-editor": "^0.19.3",
    "mousetrap": "^1.6.5",
    "ngx-color-picker": "^8.2.0",
    "ngx-monaco-editor": "^8.1.1",
    "rxjs": "6.5.4",
    "tslib": "^1.10.0",
    "web-app": "^0.1.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.6",
    "@angular-devkit/build-ng-packagr": "~0.900.6",
    "@angular/compiler-cli": "~9.0.6",
    "@types/chrome": "0.0.93",
    "@types/flat": "0.0.28",
    "@types/jasmine": "3.5.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/marked": "^0.7.2",
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^12.12.6",
    "codelyzer": "^5.2.1",
    "concurrently": "^5.0.2",
    "conventional-changelog-cli": "2.0.31",
    "flat": "^5.0.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.0",
    "karma-jasmine-html-reporter": "1.5.1",
    "ng-packagr": "^9.0.3",
    "npm-run-all": "4.1.5",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "typescript": "3.7.5"
  },
  "engines": {
    "node": ">=10.9.0"
  }
}
